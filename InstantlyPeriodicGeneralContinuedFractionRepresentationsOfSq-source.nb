Cell["\<\
This Demonstration provides a way to instantly write a periodic general \
continued fraction representation of square roots. This can be used as a \
quick way to easily approximate the numerical value of square roots. \
\>", "Text"]
}, Close]]
}, Open  ]],

Cell["\<\
(As opposed to a general continued fraction, a simple continued fraction has \
all of its numerators equal to 1.)\
\>", "Text"],

Cell[TextData[{
 "The proof of this representation is quite simple; let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"n", "+", 
     FractionBox["r", 
      RowBox[{
       RowBox[{"2", " ", "n"}], "+", 
       FractionBox["r", 
        RowBox[{
         RowBox[{"2", " ", "n"}], "+", 
         FractionBox["r", 
          RowBox[{
           RowBox[{"2", " ", "n"}], "..."}]]}]]}]]}]}], TraditionalForm]], 
  "InlineMath"],
 ". Then, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "+", "n"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"2", "n"}], "+", 
      FractionBox["r", 
       RowBox[{
        RowBox[{"2", " ", "n"}], "+", 
        FractionBox["r", 
         RowBox[{
          RowBox[{"2", " ", "n"}], "+", 
          FractionBox["r", 
           RowBox[{
            RowBox[{"2", " ", "n"}], "..."}]]}]]}]]}], "=", 
     RowBox[{
      RowBox[{"2", "n"}], "+", 
      FractionBox["r", 
       RowBox[{"x", "+", "n"}]]}]}]}], TraditionalForm]], "InlineMath"],
 ", so ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"x", "+", "n"}], ")"}], "2"], "=", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"n", "(", 
       RowBox[{"x", "+", "n"}], ")"}]}], "+", "r"}]}], TraditionalForm]], 
  "InlineMath"],
 " and simplifying gives ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["x", "2"], "=", 
     RowBox[{
      SuperscriptBox["n", "2"], "+", "r"}]}], ",", " ", 
    RowBox[{
     RowBox[{"so", " ", "x"}], "=", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["n", "2"], "+", "r"}]]}]}], TraditionalForm]], 
  "InlineMath"],
 "."
}], "Text"],

